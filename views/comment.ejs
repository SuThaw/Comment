<h1>Write Comment</h1>
<form action="/comment" method="post" class="form-inline comment" >
	<input type="text" id="message" name="message"class="form-control" placeholder="Type Comment here">	
	<input type="submit" class="btn btn-success">
</form>
<br>
<div class="list-group comment-list">
	<% comments.forEach(function(comment){%>
		<a href="" class="list-group-item">
			<%=comment.message%>

		</a>
		
	<%})%>
	
</div>






